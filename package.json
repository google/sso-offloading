{
  "name": "sso-offloading",
  "version": "0.1.0",
  "description": "Extension and TypeScript library for SSO offloading.",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm --filter \"./packages/*\" build",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "test": "vitest",
    "build:chrome-app": "pnpm --filter \"./examples/chrome-app-example\" run build",
    "build:iwa": "pnpm --filter \"./examples/simple-iwa-example\" run build",
    "build:extension": "pnpm --filter \"./packages/extension\" run build",
    "gen-iwa-update-manifest": "node ./scripts/iwa-update-manifest.js",
    "bump-version": "node ./scripts/bump-version.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/googlestaging/sso-offloading.git"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/googlestaging/sso-offloading#readme",
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/chrome": "^0.0.326",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.2.1"
  }
}
