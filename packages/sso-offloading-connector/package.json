{
  "name": "sso-offloading-connector",
  "private": false,
  "description": "A client connector for Chrome Apps and Isolated Web Apps to offload SSO to a handler extension.",
  "version": "0.1.2",
  "type": "module",
  "repository": "google/sso-offloading",
  "publishConfig": {
  "registry": "https://wombat-dressing-room.appspot.com"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "./dist/sso-offloading-connector.umd.cjs",
  "module": "./dist/sso-offloading-connector.js",
  "types": "./dist/sso-offloading-connector.d.ts",
  "exports": {
    ".": {
      "types": "./dist/sso-offloading-connector.d.ts",
      "import": "./dist/sso-offloading-connector.js",
      "require": "./dist/sso-offloading-connector.umd.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.326",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "vite": "^7.1.3"
  }
}