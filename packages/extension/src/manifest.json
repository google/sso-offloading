{
  "name": "SSO Offloading Handler",
  "description": "Handles authentication flows delegated from a trusted application.",
  "version": "1.0.21",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "webRequest",
    "webNavigation",
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "externally_connectable": {
    "ids": [
      "abaaiopbfcagdjfghfmbiifjolfnlagf"
    ],
    "matches": [
      "isolated-app://*/*"
    ]
  },
  "update_url": "http://127.0.0.1:8888/update_manifest.xml"
}