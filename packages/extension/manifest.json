{
  "name": "SSO for IWA demo",
  "description": "SSO for IWA demo",
  "version": "1.0.1",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "128": "icons/icon128.png"
  },
  "externally_connectable": {
    "matches": [
      "isolated-app://*/*"
    ],
    "ids": [
      "*"
    ]
  },
  "update_url": "http://127.0.0.1:8888/update_manifest.xml"
}