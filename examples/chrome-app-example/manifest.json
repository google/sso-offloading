{
  "manifest_version": 2,
  "name": "SSO Offloading Chrome App",
  "description": "Example Chrome App for SSO Offloading from WebView.",
  "version": "1.40",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "webview"
  ],
  "update_url": "http://127.0.0.1:8888/update_manifest.xml"
}